{
  "name": "algo",
  "version": "1.0.0",
  "description": "Javascript Library for Algorithms and DataStructures",
  "main": "lib/algo.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-register --colors -w ./test/*.spec.js",
    "test:cover": "cross-env NODE_ENV=test nyc mocha --require babel-register --colors test/*.js",
    "repl": "node -i -e \"$(< ./lib/webpack-library-starter.js)\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aa-wong/algorithms.js"
  },
  "keywords": [
    "webpack",
    "es6",
    "starter",
    "library",
    "universal",
    "umd",
    "commonjs"
  ],
  "author": "Aaron Wong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aa-wong/algorithms.js/issues"
  },
  "homepage": "https://github.com/aa-wong/algorithms.js",
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^5.2.0",
    "babel-preset-env": "^7.0.0-beta.3",
    "babel-register": "^7.0.0-beta.3",
    "chai": "^4.1.2",
    "cross-env": "^5.2.1",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.2.1",
    "jsdom": "11.11.0",
    "jsdom-global": "3.0.2",
    "mocha": "^4.0.1",
    "nyc": "^13.3.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "yargs": "^10.0.3"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  }
}
